/* -*- mode: java; tab-width: 4 -*- */

package CDPL.Chem;

/**
 * \brief AtomTypeDB.
 */
public class AtomTypeDB extends Boost.Python.instance {

	/**
	 * \brief getAtomicWeight.
	 */
	public static float getAtomicWeight(int atom_type, int isotope = 0) {}

	/**
	 * \brief isMetal.
	 */
	public static bool isMetal(int atom_type) {}

	/**
	 * \brief isNobleGas.
	 */
	public static bool isNobleGas(int atom_type) {}

	/**
	 * \brief getIUPACGroup.
	 */
	public static int getIUPACGroup(int atom_type) {}

	/**
	 * \brief getMostAbundantIsotope.
	 */
	public static int getMostAbundantIsotope(int atom_type) {}

	/**
	 * \brief isChemElement.
	 */
	public static bool isChemElement(int atom_type) {}

	/**
	 * \brief isSemiMetal.
	 */
	public static bool isSemiMetal(int atom_type) {}

	/**
	 * \brief getName.
	 */
	public static str getName(int atom_type, int isotope = 0) {}

	/**
	 * \brief isMainGroupElement.
	 */
	public static bool isMainGroupElement(int atom_type) {}

	/**
	 * \brief getVdWRadius.
	 */
	public static float getVdWRadius(int atom_type) {}

	/**
	 * \brief getAllredRochowElectronegativity.
	 */
	public static float getAllredRochowElectronegativity(int atom_type) {}

	/**
	 * \brief getSymbol.
	 */
	public static str getSymbol(int atom_type, int isotope = 0) {}

	/**
	 * \brief getType.
	 */
	public static int getType(str symbol) {}

	/**
	 * \brief isHalogen.
	 */
	public static bool isHalogen(int atom_type) {}

	/**
	 * \brief getValenceStates.
	 */
	public static object getValenceStates(int atom_type) {}

	/**
	 * \brief isTransitionMetal.
	 */
	public static bool isTransitionMetal(int atom_type) {}

	/**
	 * \brief isNonMetal.
	 */
	public static bool isNonMetal(int atom_type) {}

	/**
	 * \brief getNumValenceElectrons.
	 */
	public static int getNumValenceElectrons(int atom_type) {}
}
